#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55c3aca21460 .scope module, "tb_pwm" "tb_pwm" 2 1;
 .timescale 0 0;
v0x55c3aca38550_0 .var "clk", 0 0;
v0x55c3aca38610_0 .var "duty", 7 0;
v0x55c3aca386e0_0 .net "pwm_out", 0 0, v0x55c3aca38300_0;  1 drivers
v0x55c3aca387e0_0 .var "reset_n", 0 0;
E_0x55c3ac9e76a0 .event negedge, v0x55c3aca38300_0;
E_0x55c3aca1f700 .event posedge, v0x55c3aca38300_0;
S_0x55c3aca21630 .scope module, "uut" "pwm" 2 14, 3 1 0, S_0x55c3aca21460;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset_n";
    .port_info 2 /INPUT 8 "duty";
    .port_info 3 /OUTPUT 1 "pwm_out";
v0x55c3ac9e6490_0 .net "clk", 0 0, v0x55c3aca38550_0;  1 drivers
v0x55c3aca38130_0 .var "counter", 7 0;
v0x55c3aca38210_0 .net "duty", 7 0, v0x55c3aca38610_0;  1 drivers
v0x55c3aca38300_0 .var "pwm_out", 0 0;
v0x55c3aca383c0_0 .net "reset_n", 0 0, v0x55c3aca387e0_0;  1 drivers
E_0x55c3ac9e6f50/0 .event negedge, v0x55c3aca383c0_0;
E_0x55c3ac9e6f50/1 .event posedge, v0x55c3ac9e6490_0;
E_0x55c3ac9e6f50 .event/or E_0x55c3ac9e6f50/0, E_0x55c3ac9e6f50/1;
    .scope S_0x55c3aca21630;
T_0 ;
    %wait E_0x55c3ac9e6f50;
    %load/vec4 v0x55c3aca383c0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55c3aca38130_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55c3aca38300_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55c3aca38130_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x55c3aca38130_0, 0;
    %load/vec4 v0x55c3aca38130_0;
    %load/vec4 v0x55c3aca38210_0;
    %cmp/u;
    %jmp/0xz  T_0.2, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55c3aca38300_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55c3aca38300_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55c3aca21460;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c3aca38550_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c3aca38550_0, 0, 1;
    %delay 5, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55c3aca21460;
T_2 ;
    %wait E_0x55c3aca1f700;
    %vpi_call 2 29 "$display", "[Time %0t]: PWM output is HIGH", $time {0 0 0};
    %jmp T_2;
    .thread T_2;
    .scope S_0x55c3aca21460;
T_3 ;
    %wait E_0x55c3ac9e76a0;
    %vpi_call 2 33 "$display", "[Time %0t]: PWM output is LOW", $time {0 0 0};
    %jmp T_3;
    .thread T_3;
    .scope S_0x55c3aca21460;
T_4 ;
    %vpi_call 2 37 "$dumpfile", "waves.vcd" {0 0 0};
    %vpi_call 2 38 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55c3aca21460 {0 0 0};
    %vpi_call 2 40 "$display", "Starting PWM simulation..." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c3aca38550_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c3aca387e0_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c3aca387e0_0, 0, 1;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v0x55c3aca38610_0, 0, 8;
    %delay 3000, 0;
    %pushi/vec4 192, 0, 8;
    %store/vec4 v0x55c3aca38610_0, 0, 8;
    %delay 3000, 0;
    %pushi/vec4 64, 0, 8;
    %store/vec4 v0x55c3aca38610_0, 0, 8;
    %delay 3000, 0;
    %vpi_call 2 60 "$display", "End of PWM simulation." {0 0 0};
    %vpi_call 2 61 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_pwm.v";
    "pwm.v";
