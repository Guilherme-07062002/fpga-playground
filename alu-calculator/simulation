    %load/vec4 v0x55e82665d350_0;
    %and;
    %store/vec4 v0x55e82665d520_0, 0, 4;
    %jmp T_0.9;
T_0.3 ;
    %load/vec4 v0x55e826606490_0;
    %load/vec4 v0x55e82665d350_0;
