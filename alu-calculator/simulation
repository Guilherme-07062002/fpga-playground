    %load/vec4 v0x55e82665d350_0;
    %xor;
    %store/vec4 v0x55e82665d520_0, 0, 4;
    %jmp T_0.9;
T_0.5 ;
    %load/vec4 v0x55e826606490_0;
    %inv;
    %store/vec4 v0x55e82665d520_0, 0, 4;
    %jmp T_0.9;
T_0.6 ;
    %load/vec4 v0x55e826606490_0;
    %load/vec4 v0x55e82665d350_0;
    %cmp/u;
    %flag_mov 8, 5;
    %jmp/0 T_0.10, 8;
    %pushi/vec4 1, 0, 4;
    %jmp/1 T_0.11, 8;
T_0.10 ; End of true expr.
    %pushi/vec4 0, 0, 4;
    %jmp/0 T_0.11, 8;
 ; End of false expr.
    %blend;
T_0.11;
    %store/vec4 v0x55e82665d520_0, 0, 4;
    %jmp T_0.9;
T_0.7 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55e82665d520_0, 0, 4;
    %jmp T_0.9;
T_0.9 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55e826641c40;
T_1 ;
