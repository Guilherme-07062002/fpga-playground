    %vpi_call 2 78 "$display", "A=%d, B=%d, operation=NOP, result=%d", v0x55b8c3714dc0_0, v0x55b8c3714ea0_0, v0x55b8c3715070_0 {0 0 0};
    %vpi_call 2 80 "$display", "End of ALU simulation." {0 0 0};
    %vpi_call 2 81 "$finish" {0 0 0};
