S_0x55b8c36eb0a0 .scope module, "tb_alu" "tb_alu" 2 2;
 .timescale 0 0;
v0x55b8c3714dc0_0 .var "A", 3 0;
v0x55b8c3714ea0_0 .var "B", 3 0;
v0x55b8c3714f70_0 .var "operation", 2 0;
v0x55b8c3715070_0 .net "result", 3 0, v0x55b8c3714c30_0;  1 drivers
S_0x55b8c36fac40 .scope module, "uut" "alu" 2 17, 3 2 0, S_0x55b8c36eb0a0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "A";
    .port_info 1 /INPUT 4 "B";
    .port_info 2 /INPUT 3 "operation";
    .port_info 3 /OUTPUT 4 "result";
v0x55b8c36faeb0_0 .net "A", 3 0, v0x55b8c3714dc0_0;  1 drivers
v0x55b8c3714a60_0 .net "B", 3 0, v0x55b8c3714ea0_0;  1 drivers
v0x55b8c3714b40_0 .net "operation", 2 0, v0x55b8c3714f70_0;  1 drivers
v0x55b8c3714c30_0 .var "result", 3 0;
E_0x55b8c36bfa70 .event anyedge, v0x55b8c3714b40_0, v0x55b8c36faeb0_0, v0x55b8c3714a60_0;
    .scope S_0x55b8c36fac40;
T_0 ;
    %wait E_0x55b8c36bfa70;
    %load/vec4 v0x55b8c3714b40_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 3;
    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.0 ;
    %load/vec4 v0x55b8c36faeb0_0;
    %load/vec4 v0x55b8c3714a60_0;
    %add;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.1 ;
    %load/vec4 v0x55b8c36faeb0_0;
    %load/vec4 v0x55b8c3714a60_0;
    %sub;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.2 ;
    %load/vec4 v0x55b8c36faeb0_0;
    %load/vec4 v0x55b8c3714a60_0;
    %and;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.3 ;
    %load/vec4 v0x55b8c36faeb0_0;
    %load/vec4 v0x55b8c3714a60_0;
    %or;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.4 ;
    %load/vec4 v0x55b8c36faeb0_0;
    %load/vec4 v0x55b8c3714a60_0;
    %xor;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.5 ;
    %load/vec4 v0x55b8c36faeb0_0;
    %inv;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.6 ;
    %load/vec4 v0x55b8c36faeb0_0;
    %load/vec4 v0x55b8c3714a60_0;
    %cmp/u;
    %flag_mov 8, 5;
    %jmp/0 T_0.10, 8;
    %pushi/vec4 1, 0, 4;
    %jmp/1 T_0.11, 8;
T_0.10 ; End of true expr.
    %pushi/vec4 0, 0, 4;
    %jmp/0 T_0.11, 8;
 ; End of false expr.
    %blend;
T_0.11;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.7 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55b8c3714c30_0, 0, 4;
    %jmp T_0.9;
T_0.9 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55b8c36eb0a0;
T_1 ;
    %vpi_call 2 29 "$dumpfile", "waves.vcd" {0 0 0};
    %vpi_call 2 31 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55b8c36eb0a0 {0 0 0};
    %vpi_call 2 34 "$display", "Starting ALU simulation..." {0 0 0};
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x55b8c3714dc0_0, 0, 4;
