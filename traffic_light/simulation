S_0x559dcaa15490 .scope module, "tb_traffic_light" "tb_traffic_light" 2 1;
 .timescale 0 0;
v0x559dcaa6dc60_0 .var "clk", 0 0;
v0x559dcaa6dd20_0 .net "green", 0 0, L_0x559dcaa6e170;  1 drivers
v0x559dcaa6ddc0_0 .net "red", 0 0, L_0x559dcaa6e030;  1 drivers
v0x559dcaa6de90_0 .var "reset_n", 0 0;
v0x559dcaa6df60_0 .net "yellow", 0 0, L_0x559dcaa6e300;  1 drivers
E_0x559dcaa53880 .event posedge, v0x559dcaa6d6b0_0;
E_0x559dcaa53f90 .event posedge, v0x559dcaa6db00_0;
E_0x559dcaa53bf0 .event posedge, v0x559dcaa6d7c0_0;
S_0x559dcaa15640 .scope module, "uut" "traffic_light" 2 17, 3 1 0, S_0x559dcaa15490;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset_n";
    .port_info 2 /OUTPUT 1 "red";
    .port_info 3 /OUTPUT 1 "yellow";
