    %vpi_call 2 47 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559dcaa15490 {0 0 0};
    %vpi_call 2 49 "$display", "Starting traffic light simulation..." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559dcaa6dc60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559dcaa6de90_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559dcaa6de90_0, 0, 1;
    %delay 2000, 0;
    %vpi_call 2 57 "$display", "End of traffic light simulation." {0 0 0};
    %vpi_call 2 58 "$finish" {0 0 0};
    %end;
