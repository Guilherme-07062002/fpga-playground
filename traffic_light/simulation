v0x559dcaa6d530_0 .net/2u *"_ivl_8", 1 0, L_0x7fd0dbe880a8;  1 drivers
v0x559dcaa6d5f0_0 .net "clk", 0 0, v0x559dcaa6dc60_0;  1 drivers
v0x559dcaa6d6b0_0 .net "green", 0 0, L_0x559dcaa6e170;  alias, 1 drivers
v0x559dcaa6d7c0_0 .net "red", 0 0, L_0x559dcaa6e030;  alias, 1 drivers
v0x559dcaa6d880_0 .net "reset_n", 0 0, v0x559dcaa6de90_0;  1 drivers
v0x559dcaa6d940_0 .var "state", 1 0;
v0x559dcaa6da20_0 .var "timer", 5 0;
v0x559dcaa6db00_0 .net "yellow", 0 0, L_0x559dcaa6e300;  alias, 1 drivers
E_0x559dcaa53a70/0 .event negedge, v0x559dcaa6d880_0;
E_0x559dcaa53a70/1 .event posedge, v0x559dcaa6d5f0_0;
E_0x559dcaa53a70 .event/or E_0x559dcaa53a70/0, E_0x559dcaa53a70/1;
L_0x559dcaa6e030 .cmp/eq 2, v0x559dcaa6d940_0, L_0x7fd0dbe88018;
L_0x559dcaa6e170 .cmp/eq 2, v0x559dcaa6d940_0, L_0x7fd0dbe88060;
L_0x559dcaa6e300 .cmp/eq 2, v0x559dcaa6d940_0, L_0x7fd0dbe880a8;
    .scope S_0x559dcaa15640;
T_0 ;
    %wait E_0x559dcaa53a70;
    %load/vec4 v0x559dcaa6da20_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x559dcaa6da20_0, 0;
    %load/vec4 v0x559dcaa6d880_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x559dcaa6d940_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x559dcaa6da20_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x559dcaa6d940_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x559dcaa6d940_0, 0;
    %jmp T_0.6;
T_0.2 ;
    %load/vec4 v0x559dcaa6da20_0;
    %pad/u 32;
    %cmpi/e 49, 0, 32;
    %jmp/0xz  T_0.7, 4;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x559dcaa6d940_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x559dcaa6da20_0, 0;
T_0.7 ;
    %jmp T_0.6;
T_0.3 ;
    %load/vec4 v0x559dcaa6da20_0;
    %pad/u 32;
    %cmpi/e 49, 0, 32;
    %jmp/0xz  T_0.9, 4;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x559dcaa6d940_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x559dcaa6da20_0, 0;
T_0.9 ;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x559dcaa6da20_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_0.11, 4;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x559dcaa6d940_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x559dcaa6da20_0, 0;
T_0.11 ;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x559dcaa15490;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559dcaa6dc60_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559dcaa6dc60_0, 0, 1;
    %delay 5, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x559dcaa15490;
T_2 ;
    %wait E_0x559dcaa53bf0;
    %vpi_call 2 34 "$display", "[Time %0t]: RED light is ON", $time {0 0 0};
    %jmp T_2;
    .thread T_2;
    .scope S_0x559dcaa15490;
T_3 ;
    %wait E_0x559dcaa53f90;
    %vpi_call 2 38 "$display", "[Time %0t]: YELLOW light is ON", $time {0 0 0};
    %jmp T_3;
    .thread T_3;
    .scope S_0x559dcaa15490;
T_4 ;
    %wait E_0x559dcaa53880;
    %vpi_call 2 42 "$display", "[Time %0t]: GREEN light is ON", $time {0 0 0};
    %jmp T_4;
    .thread T_4;
    .scope S_0x559dcaa15490;
T_5 ;
