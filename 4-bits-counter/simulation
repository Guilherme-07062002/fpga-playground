#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x559d5d4e9160 .scope module, "tb_counter" "tb_counter" 2 3;
 .timescale -9 -12;
v0x559d5d4fbf10_0 .var "clk", 0 0;
v0x559d5d4fbfe0_0 .net "out", 3 0, v0x559d5d4fbd10_0;  1 drivers
v0x559d5d4fc0b0_0 .var "rst_n", 0 0;
S_0x559d5d4e92f0 .scope module, "uut" "counter" 2 11, 3 1 0, S_0x559d5d4e9160;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 4 "out";
v0x559d5d4e9540_0 .net "clk", 0 0, v0x559d5d4fbf10_0;  1 drivers
v0x559d5d4fbd10_0 .var "out", 3 0;
v0x559d5d4fbdf0_0 .net "rst_n", 0 0, v0x559d5d4fc0b0_0;  1 drivers
E_0x559d5d4aff10/0 .event negedge, v0x559d5d4fbdf0_0;
E_0x559d5d4aff10/1 .event posedge, v0x559d5d4e9540_0;
E_0x559d5d4aff10 .event/or E_0x559d5d4aff10/0, E_0x559d5d4aff10/1;
    .scope S_0x559d5d4e92f0;
T_0 ;
    %wait E_0x559d5d4aff10;
    %load/vec4 v0x559d5d4fbdf0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x559d5d4fbd10_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x559d5d4fbd10_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x559d5d4fbd10_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x559d5d4e9160;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559d5d4fbf10_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x559d5d4fbf10_0;
    %inv;
    %store/vec4 v0x559d5d4fbf10_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x559d5d4e9160;
T_2 ;
    %vpi_call 2 26 "$dumpfile", "waves.vcd" {0 0 0};
    %vpi_call 2 27 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559d5d4e9160 {0 0 0};
    %vpi_call 2 30 "$display", "Iniciando Simula\303\247\303\243o..." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559d5d4fc0b0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559d5d4fc0b0_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559d5d4fc0b0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559d5d4fc0b0_0, 0, 1;
    %delay 50000, 0;
    %vpi_call 2 43 "$display", "Fim da Simula\303\247\303\243o." {0 0 0};
    %vpi_call 2 44 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_counter.v";
    "counter.v";
